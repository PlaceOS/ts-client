{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build && npm run docs",
    "docs": "typedoc --excludePrivate --out docs --target es6 --theme minimal --mode file src",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch",
    "test:coverage": "tsdx test --coverage",
    "test:ci": "tsdx test --runInBand",
    "lint": "tsdx lint",
    "prepare": "tsdx build && node ./copy-file.js"
  },
  "peerDependencies": {
    "date-fns": "^2.15.0",
    "rxjs": "^6.6.2",
    "ts-md5": "^1.2.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "name": "@placeos/ts-client",
  "author": "Alex Sorafumo",
  "module": "dist/ts-client2.esm.js",
  "devDependencies": {
    "date-fns": "^2.15.0",
    "husky": "^4.2.5",
    "rxjs": "^6.6.2",
    "semantic-release": "^17.1.1",
    "ts-md5": "^1.2.7",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.1",
    "typedoc": "^0.17.8",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "byte-base64": "^1.0.2",
    "fast-sha256": "^1.3.0"
  }
}
